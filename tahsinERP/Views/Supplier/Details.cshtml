@model tahsinERP.Models.SUPPLIERS
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="app-content content">
    <div class="content-overlay"></div>
    <div class="content-wrapper">
        <div class="content-header row">
            <div class="content-header-left col-12 mb-2 mt-1">
                <div class="row breadcrumbs-top">
                    <div class="col-12">
                        <div class="breadcrumb-wrapper col-12">
                            <ol class="breadcrumb rounded-pill breadcrumb-divider">
                                <li class="breadcrumb-item">
                                    <a href="~/Home"><i class="bx bx-home-alt"></i></a>
                                </li>
                                <li class="breadcrumb-item ">
                                    <a href="~/Supplier">Ta'minotchilar</a>
                                </li>
                                <li class="breadcrumb-item active">
                                    @Model.Name malumotlari
                                </li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="content-body">
            <!-- app invoice View Page -->
            <section class="invoice-edit-wrapper">
                <div class="row">
                    <!-- invoice view page -->
                    <div class="col-xl-9 col-md-8 col-12">
                        <div class="card">
                            <div class="card-content">
                                <div class="card-body pb-0 mx-25">
                                    <div class="row invoice-info">
                                        <div class="col-lg-12 col-md-6 mt-25">
                                            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                                            <hr>
                                            <h6 class="invoice-to">Nomi</h6>
                                            <fieldset class="invoice-address form-group">
                                                @Html.DisplayFor(model => model.Name, new { @class = "form-control" })
                                            </fieldset>
                                            <h6 class="invoice-to">DUNS</h6>
                                            <fieldset class="invoice-address form-group">
                                                @Html.DisplayFor(model => model.DUNS, new { @class = "form-control" })
                                            </fieldset>
                                            <h6 class="invoice-to">Turi</h6>
                                            <fieldset class="invoice-address form-group">
                                                @Html.DisplayFor(model => model.Type, new { @class = "form-control" })
                                            </fieldset>
                                            <h6 class="invoice-to">Mamlakati</h6>
                                            <fieldset class="invoice-address form-group">
                                                @Html.DisplayFor(model => model.Country, new { @class = "form-control" })
                                            </fieldset>
                                            <h6 class="invoice-to">Shahri</h6>
                                            <fieldset class="invoice-address form-group">
                                                @Html.DisplayFor(model => model.City, new { @class = "form-control" })
                                            </fieldset>
                                            <h6 class="invoice-to">Manzili</h6>
                                            <fieldset class="invoice-address form-group">
                                                @Html.DisplayFor(model => model.Address, new { @class = "form-control" })
                                            </fieldset>
                                            <h6 class="invoice-to">Telefon raqami</h6>
                                            <fieldset class="invoice-address form-group">
                                                @Html.DisplayFor(model => model.Telephone, new { @class = "form-control" })
                                            </fieldset>
                                            <h6 class="invoice-to">Elektron manzili</h6>
                                            <fieldset class="invoice-address form-group">
                                                @Html.DisplayFor(model => model.Email, new { @class = "form-control" })
                                            </fieldset>
                                            <h6 class="invoice-to">Aloqa uchun hodim</h6>
                                            <fieldset class="invoice-address form-group">
                                                @Html.DisplayFor(model => model.ContactPersonName, new { @class = "form-control" })
                                            </fieldset>
                                            <h6 class="invoice-to">Direktor ismi</h6>
                                            <fieldset class="invoice-address form-group">
                                                @Html.DisplayFor(model => model.DirectorName, new { @class = "form-control" })
                                            </fieldset>
                                        </div>
                                    </div>
                                    <hr>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </div>
    </div>
</div>